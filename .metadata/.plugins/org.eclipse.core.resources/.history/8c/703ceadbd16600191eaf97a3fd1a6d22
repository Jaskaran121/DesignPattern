package VisitorPatternTutorialsPoint;

public class VisitorA implements IVisitor{

	@Override
	public void visit(IDefaultClass defaultClass) {
		// TODO Auto-generated method stub
		if(defaultClass.getClass().equals(DefaultClassA.class))
		{
			System.out.println("Applying visitor algorithm to Defaule Class A");
		}
		else if(defaultClass.getClass().equals(DefaultClassB.class))
			System.out.println("Applying visitor algorithm to Defaule Class B");
		else
			System.out.println("Applying visitor algorithm to Defaule Class C");
		
	}

}
